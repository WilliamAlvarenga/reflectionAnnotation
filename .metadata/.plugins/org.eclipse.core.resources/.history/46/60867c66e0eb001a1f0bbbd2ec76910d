package reflectionAnnotation;

import java.math.BigDecimal;
import java.util.Map;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Product p = new Product("Design Patterns","Publicado pela Casa do Codigo",BigDecimal.valueOf(59.90),
				"LIVRO");
				Map<String,Object> props = GeradorMapa.gerarMapa(p);
				for(String prop : props.keySet()){
				System.out.println(prop+" = "+props.get(prop));
				}

		
		
	}

}
